                def __init__(self, path, overwrite):
                    self.path = pycompat.text_type(path)
                    self.overwrite = overwrite
            
                def write(self, s):
                    with open(self.path, 'w' if self.overwrite else 'a',
                              encoding='utf-8') as output_file:
                        output_file.write(s)
                    self.overwrite = False

