import pytest
from pypara.monetary import Price

class ConcretePrice(Price):
    def __int__(self):
        return super().__int__()

def test_price_int_implementation(mocker):
    mocker.patch.object(Price, '__int__', return_value=42)
    price = ConcretePrice()
    assert int(price) == 42
