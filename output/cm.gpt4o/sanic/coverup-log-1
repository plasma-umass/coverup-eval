def test_fwd_normalize():
    # Test data
    fwd = [
        ("by", "192.168.0.1"),
        ("for", "192.168.0.2"),
        ("host", "EXAMPLE.COM"),
        ("proto", "HTTP"),
        ("port", "8080"),
        ("path", "/some%20path"),
        ("unknown", "value"),
        ("port", "invalid_port"),  # This should trigger ValueError and be ignored
    ]

    # Expected result
    expected = {
        "by": "192.168.0.1",
        "for": "192.168.0.2",
        "host": "example.com",
        "proto": "http",
        "port": 8080,
        "path": "/some path",
        "unknown": "value"
    }

    # Call the function
    result = fwd_normalize(fwd)

    # Assertions
    assert result == expected

