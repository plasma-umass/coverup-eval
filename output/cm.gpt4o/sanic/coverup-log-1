```python
import pytest
from sanic.mixins.routes import RouteMixin
from sanic.views import HTTPMethodView, CompositionView
from sanic import Sanic
from unittest.mock import Mock

class TestRouteMixin(RouteMixin):
    def __init__(self):
        self.strict_slashes = True
        self.route = Mock()

@pytest.fixture
def app():
    return TestRouteMixin()

def test_add_route_with_httpmethodview(app):
    class MyView(HTTPMethodView):
        def get(self, request):
            return "GET"
        def post(self, request):
            return "POST"

    handler = MyView()
    app.add_route(handler, "/test")

    app.route.assert_called_once_with(
        uri="/test",
        methods={"GET", "POST"},
        host=None,
        strict_slashes=True,
        stream=False,
        version=None,
        name=None,
    )

def test_add_route_with_compositionview(app):
    def handler1(request):
        return "handler1"
    def handler2(request):
        return "handler2"

    view = CompositionView()
    view.add(["GET"], handler1)
    view.add(["POST"], handler2)

    app.add_route(view, "/test")

    app.route.assert_called_once_with(
        uri="/test",
        methods={"GET", "POST"},
        host=None,
        strict_slashes=True,
        stream=False,
        version=None,
        name=None,
    )

def test_add_route_with_strict_slashes(app):
    handler = Mock()
    app.add_route(handler, "/test", strict_slashes=False)

    app.route.assert_called_once_with(
        uri="/test",
        methods={"GET"},
        host=None,
        strict_slashes=False,
        stream=False,
        version=None,
        name=None,
    )

def test_add_route_with_stream_handler(app):
    handler = Mock()
    handler.is_stream = True
    app.add_route(handler, "/test")

    app.route.assert_called_once_with(
        uri="/test",
        methods={"GET"},
        host=None,
        strict_slashes=True,
        stream=True,
        version=None,
        name=None,
    )
```
