    assert bump_version('1.2.0a0', pre_release='b') == '1.2.0b0'
    assert bump_version('1.2.0b0', pre_release='b') == '1.2.0b1'
    
    # Test lines 283-289: ver_info.patch.pre_txt == 'b'
    assert bump_version('1.2.3b0', pre_release='b') == '1.2.3b1'

