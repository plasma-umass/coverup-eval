    _PyInfo.PY2 = True
    _PyInfo.PY3 = False

    # Access the attributes to ensure they are set
    assert _PyInfo.string_types == (basestring,)
    assert _PyInfo.text_type == unicode
    assert _PyInfo.binary_type == str
