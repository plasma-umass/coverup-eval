            class RoughParser:
                def _study1(self):
                    # pylint: disable=redefined-builtin
            
       258:         if self.study_level >= 1:
       259:             return
       260:         self.study_level = 1
            
                    # Map all uninteresting characters to "x", all open brackets
                    # to "(", all close brackets to ")", then collapse runs of
                    # uninteresting characters.  This can cut the number of chars
                    # by a factor of 10-40, and so greatly speed the following loop.
       266:         str = (
       267:             self.str.translate(self._tran)  # @ReservedAssignment
       268:             .replace("xxxxxxxx", "x")
       269:             .replace("xxxx", "x")
       270:             .replace("xx", "x")
       271:             .replace("xx", "x")
       272:             .replace("\nx", "\n")
                    )
                    # note that replacing x\n with \n would be incorrect, because
                    # x may be preceded by a backslash
            
                    # March over the squashed version of the program, accumulating
                    # the line numbers of non-continued stmts, and determining
                    # whether & why the last stmt is a continuation.
       280:         continuation = C_NONE
       281:         level = lno = 0  # level is nesting level; lno is line number
       282:         self.goodlines = goodlines = [0]
       283:         push_good = goodlines.append
       284:         i, n = 0, len(str)
       285:         while i < n:
       286:             ch = str[i]
       287:             i = i + 1
            
                        # cases are checked in decreasing order of frequency
       290:             if ch == "x":
       291:                 continue
            
       293:             if ch == "\n":
       294:                 lno = lno + 1
       295:                 if level == 0:
       296:                     push_good(lno)
                                # else we're in an unclosed bracket structure
       298:                 continue
            
       300:             if ch == "(":
       301:                 level = level + 1
       302:                 continue
            
       304:             if ch == ")":
       305:                 if level:
       306:                     level = level - 1
                                # else the program is invalid, but we can't complain
       308:                 continue
            
       310:             if ch == '"' or ch == "'":
                            # consume the string
       312:                 quote = ch
       313:                 if str[i - 1 : i + 2] == quote * 3:
       314:                     quote = quote * 3
       315:                 firstlno = lno
       316:                 w = len(quote) - 1
       317:                 i = i + w
       318:                 while i < n:
       319:                     ch = str[i]
       320:                     i = i + 1
            
       322:                     if ch == "x":
       323:                         continue
            
       325:                     if str[i - 1 : i + w] == quote:
       326:                         i = i + w
       327:                         break
            
       329:                     if ch == "\n":
       330:                         lno = lno + 1
       331:                         if w == 0:
                                        # unterminated single-quoted string
                                        # It doesn't matter if we're in brackets,
                                        # this should lead to
                                        # SyntaxError: EOL while scanning string literal
       336:                             level = 0
       337:                             push_good(lno)
       338:                             break
       339:                         continue
            
       341:                     if ch == "\\":
       342:                         assert i < n
       343:                         if str[i] == "\n":
       344:                             lno = lno + 1
       345:                         i = i + 1
       346:                         continue
            
                                # else comment char or paren inside string
            
                            else:
                                # didn't break out of the loop, so we're still
                                # inside a string
       353:                     if (lno - 1) == firstlno:
                                    # before the previous \n in str, we were in the first
                                    # line of the string
       356:                         continuation = C_STRING_FIRST_LINE
                                else:
       358:                         continuation = C_STRING_NEXT_LINES
       359:                 continue  # with outer loop
            
       361:             if ch == "#":
                            # consume the comment
       363:                 i = str.find("\n", i)
       364:                 assert i >= 0
       365:                 continue
            
       367:             assert ch == "\\"
       368:             assert i < n
       369:             if str[i] == "\n":
       370:                 lno = lno + 1
       371:                 if i + 1 == n:
       372:                     continuation = C_BACKSLASH
       373:             i = i + 1
            
                    # The last stmt may be continued for all 3 reasons.
                    # String continuation takes precedence over bracket
                    # continuation, which beats backslash continuation.
       378:         if (
       379:             continuation != C_STRING_FIRST_LINE
       380:             and continuation != C_STRING_NEXT_LINES
       381:             and level > 0
                    ):
       383:             continuation = C_BRACKET
       384:         self.continuation = continuation
            
                    # Push the final line number as a sentinel value, regardless of
                    # whether it's continued.
       388:         assert (continuation == C_NONE) == (goodlines[-1] == lno)
       389:         if goodlines[-1] != lno:
       390:             push_good(lno)

