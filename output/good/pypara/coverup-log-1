import pytest
from pypara.accounting.journaling import JournalEntry
from typing import Generic, TypeVar, Iterable
from dataclasses import dataclass

_T = TypeVar('_T')

@dataclass
class Posting(Generic[_T]):
    amount: _T
    is_credit: bool

