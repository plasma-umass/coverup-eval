@dataclass
class Posting(Generic[_T]):
    amount: _T
    is_credit: bool

@pytest.fixture
def mock_postings():
    return [
        Posting(amount=100, is_credit=True),
        Posting(amount=200, is_credit=False),
        Posting(amount=300, is_credit=True)
    ]

def test_journal_entry_credits(mock_postings):
