```python
import pytest
from collections.abc import Mapping
from typesystem.schemas import SchemaMetaclass

class TestSchema(Mapping, metaclass=SchemaMetaclass):
    fields = {
        'field1': str,
        'field2': int,
        'field3': bool
    }

    def __init__(self):
        self.field1 = "test"
        self.field2 = 123

    def __getitem__(self, key):
        return getattr(self, key)

    def __iter__(self):
        return iter(self.fields)

    def __len__(self):
        return len([key for key in self.fields if hasattr(self, key)])

def test_schema_len():
    schema = TestSchema()
    assert len(schema) == 2  # Only field1 and field2 should be counted

    # Clean up
    del schema.field1
    del schema.field2
```
