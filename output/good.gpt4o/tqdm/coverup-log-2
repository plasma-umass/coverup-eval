    tqdm_instance.pos = 1
    tqdm_instance.close()
    mock_tgio.delete.assert_not_called()

def test_tqdm_telegram_delete_not_called_when_leave_none_and_pos_not_zero(tqdm_instance, mock_tgio):
    tqdm_instance.leave = None
    tqdm_instance.pos = 1
    tqdm_instance.close()
