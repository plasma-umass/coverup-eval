    cookie = Cookie()
    expected_keys = {
        "expires": "expires",
        "path": "Path",
        "comment": "Comment",
        "domain": "Domain",
        "max-age": "Max-Age",
        "secure": "Secure",
        "httponly": "HttpOnly",
        "version": "Version",
        "samesite": "SameSite",
    }
    assert cookie._keys == expected_keys

def test_cookie_flags():
    cookie = Cookie()
    expected_flags = {"secure", "httponly"}
    assert cookie._flags == expected_flags
